#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    int i,x=0,j,l=0,a=0;
    char s[1000];
    scanf("%s",s);
    for(i=0;i<10;i++)
    {
        for(j=0;j<=strlen(s);j++)
        {
            if( (s[j]-'0') ==i)
            {
                l=l+1;
            }
        }
        printf("%d ",l);
        l=0;
    }

    return 0;
}
