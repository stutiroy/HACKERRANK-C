#include<stdio.h>
int sum(int n,int a,int b,int c)
{
    if (n==1)
     return a;
     if (n==2)
     return b;
     if (n==3)
     return c;
     else {
       return (n-1)+(n-2)+(n-3);
     }


}
int main()
{
    int k,a1,a2,a3;
    scanf("%d,%d,%d,%d",&k,&a1,&a2,&a3);
    sum(k,a1,a2,a3);
    printf("%d",sum);
}

